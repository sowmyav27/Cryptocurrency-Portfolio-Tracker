<body class="">
    <div class="wrapper">
        <div class="sidebar" data-color="purple" data-background-color="white">
      
          <div class="logo">
                <a href="#" class="simple-text logo-normal">
                    Menu
                </a>
            </div>
            <div class="sidebar-wrapper">
                <ul class="nav">
                    <li class="nav-item ">
                        <a class="nav-link" href="/home">
                            <i class="material-icons">dashboard</i>
                            <p>Dashboard</p>
                        </a>
                    </li>
                    <li class="nav-item ">
                        <a class="nav-link" href="/addwallet">
                            <i class="fa fa-credit-card"></i>
                            <p>Add to Wallet</p>
                        </a>
                    </li>	
                    <li class="nav-item active ">
                        <a class="nav-link" href="/myWallet">
                            <i class="fa fa-credit-card"></i>
                            <p>My Wallet</p>
                        </a>
                    </li>
					<li class="nav-item  ">
                        <a class="nav-link" href="/history">
                            <i class="fa fa-history"></i>
                            <p>My History</p>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
		
	<div class="main-panel">
	 <!-- Start Navbar -->
	 <nav class="navbar navbar-expand-lg navbar-transparent  navbar-absolute fixed-top">
                <div class="container-fluid">
                  
                    <div class="collapse navbar-collapse justify-content-end" id="navigation">
                        <ul class="navbar-nav">
                           <li class="nav-item">	
								<a class="item" href="/about">About Us</a>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link" href="" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="material-icons">person</i>
                                   
                                </a>
                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
                                    <a class="dropdown-item" href="/viewinfo">My Profile</a>
                                    <a class="dropdown-item" href="/updateuser">Update Profile</a>
									<a class ="dropdown-item" href="/">Logout</a>
                                </div>
                            </li>
                            
                        </ul>
                    </div>
                </div>
            </nav>
	 
	 <!-- End Navbar -->
	 
	 
				<div class="content">
		 <div class="container-fluid">
            <div class="row">
				<div class="col-md-12">
                            <div class="card">
                                <div class="card-header card-header-primary">
								<h2 class="card-title">My Wallet</h2>        
									 </div>
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <table class="table table-striped">		
											<thead class = "text-primary">
										<tr>
														
													<th>#</th>
													<th>Currency</th>
													<th>Order Date</th>
													<th>Order Quantity</th>
													<th>Total Amount</th>
													<th>Payment Status</th>
													</tr>
									</thead>
								<tbody>
									<% for(var i=0; i < mywalletlist.length; i++) { %>
														<tr>
													
														<th><%= i+1 %></th>
														
														<td><b><%= mywalletlist[i].currency_code%></b></td>
														<td><b><%= mywalletlist[i].order_date%></b></td>
														<td><b><%= mywalletlist[i].order_qty%></b></td>
														<td><b><%= mywalletlist[i].tot_amt%></b></td>
														<td><b><%= mywalletlist[i].payment_status%></b></td>
														</tr>
														<% } %>
								</tbody>
									</table>
		<form id="form_id" method="post" action="/mywallet">
		<input type="submit" class="btn btn-primary pull-right" value="Buy" id="submit"/></form>
		</div>
		</div>
		</div>
		</div>
		</div>
		</div>
		</div>
		</div>
		</div>
	 
